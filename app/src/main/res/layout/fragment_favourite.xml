            android:layout_marginEnd="5dp"
            android:id="@+id/recyclerviewmain"
            tools:listitem="@layout/recyclerview"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp" />




</androidx.constraintlayout.widget.ConstraintLayout>